{
  "name": "linux-command",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "pnpm run generate:index && vite",
    "build": "pnpm run generate:index && run-p type-check \"build-only {@}\" --",
    "preview": "pnpm run generate:index && vite preview",
    "build-only": "pnpm run generate:index && vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "oxlint .",
    "generate:index": "node scripts/generate-index.cjs",
    "predev": "pnpm run generate:index",
    "prebuild": "pnpm run generate:index"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "@vueuse/core": "^14.1.0",
    "fuse.js": "^7.1.0",
    "highlight.js": "^11.11.1",
    "marked": "^17.0.1",
    "marked-highlight": "^2.2.3",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "vuetify": "^3.10.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^1.41.0",
    "sass-embedded": "^1.92.1",
    "typescript": "~5.9.2",
    "unplugin-fonts": "^1.4.0",
    "unplugin-vue-components": "^29.0.0",
    "unplugin-vue-router": "^0.15.0",
    "vite": "^7.1.5",
    "vite-plugin-vuetify": "^2.1.2",
    "vue-tsc": "^3.0.7"
  }
}
